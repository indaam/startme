include jade-libs/asset/mixin

+head("home", "http://fonts.googleapis.com/css?family=Droid+Sans:400,700|Raleway:400,300,500,600,800,700,900")

body
  +ie_notice

  header#page-header.header-one(role='banner')
    nav#top-nav
      .container
        include jade-libs/element/nav-mini
        include jade-libs/element/nav-account
    // End #top-nav

    #header
      .container
        include jade-libs/element/header-title
        include jade-libs/element/navbar
    // End #header
  
  include jade-libs/element/learn/featured

  section#page-content
    .container
      main#page-main.page-main(role="main")
        #content
          include jade-libs/element/learn/intro
          include jade-libs/element/learn/banner

          // popular-products
          section.products-wrap.popular-products.cl
            h2.title-section
              span featured Product
            ul.wrap-item-list.products
              +product_view("")
              +product_view("")
              +product_view("")
              +product_view("")

          // products-slider
          section.products-wrap.products-slider.cl
            h2.title-section
              span recent products
            ul.wrap-item-list.products.use-slider
