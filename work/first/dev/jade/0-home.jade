            h2.title-section
              span Products Mini
            ul.wrap-item-list.products.products-mini
              +product_view("")
              +product_view("")
              +product_view("")
              +product_view("")
          section.home-features.cl
            .container
              include jade-libs/element/feature-testimonial
              include jade-libs/element/feature-subscribe

          // filter-product
          section.products-wrap.filter-product.cl
            h2.title-section
              span Products Filter
            ul.button-filter.add-clearfix.reset-list
              li.active(data-value='all')
                a(href='#') All
              li(data-value='tsirt')
                a(href='#') t-sirt
              li(data-value='jeans')
                a(href='#') Jeans
              li(data-value='bags')
                a(href='#') Bags
            ul.wrap-item-list.products.products-mini
              +product_view("tsirt")
              +product_view("bags")
              +product_view("jeans")
              +product_view("tsirt")
              +product_view("tsirt")
              +product_view("bags")
